@model LaptopModel

@{
   
}

<!DOCTYPE html>

<html>
<head>
    <title>Add Router</title>
</head>
<body>
<div>
    <form action="AddLaptop" method="post">
        <div class="form-group">
            <input name="Processor" required placeholder="Processor" class="form-control"/>
        </div>
        <div class="form-group">
            <input name="HD" required placeholder="HD" class="form-control"/>
        </div>
        <div class="form-group">
            <input name="Display" required placeholder="Display" class="form-control"/>
        </div>
        <div class="form-group">
            <input name="IP" required placeholder="IP" class="form-control"/>
        </div>
        <div class="form-group">
            <input name="Brand" required placeholder="Brand" class="form-control"/>
        </div>
        <div class="form-group">
            <input name="User" required placeholder="User" class="form-control"/>
        </div>
        <div class="form-group">
            <input name="Ram" required placeholder="Ram" class="form-control"/>
        </div>
        <div class="form-group">
            <input name="DeviceName" required placeholder="DeviceName" class="form-control"/>
        </div>
        <div class="form-group">
            <input name="SerialNo" required placeholder="SerialNo" class="form-control"/>
        </div>
        <div class="form-group">
            <input name="Memo" required placeholder="Memo" class="form-control"/>
        </div>
        <div class="form-group">
            <input name="AcquisitionDate" required type="date" placeholder="AcquisitionDate" class="form-control"/>
        </div>
        
        <input type="submit" class="btn btn-success"/>
    </form>
</div>
</body>
</html>